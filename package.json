{
  "name": "data-flow-map-book-site",
  "version": "1.0.0",
  "description": "Static site for The Data Flow Map book by Nick Ryberg",
  "scripts": {
    "start": "eleventy --serve",
    "start:drafts": "DRAFTS=true eleventy --serve",
    "start:style1": "STYLE=1 eleventy --serve",
    "start:style2": "STYLE=2 eleventy --serve",
    "start:style3": "STYLE=3 eleventy --serve",
    "start:style4": "STYLE=4 eleventy --serve",
    "start:style5": "STYLE=5 eleventy --serve",
    "start:style1:drafts": "STYLE=1 DRAFTS=true eleventy --serve",
    "start:style2:drafts": "STYLE=2 DRAFTS=true eleventy --serve",
    "start:style3:drafts": "STYLE=3 DRAFTS=true eleventy --serve",
    "start:style4:drafts": "STYLE=4 DRAFTS=true eleventy --serve",
    "start:style5:drafts": "STYLE=5 DRAFTS=true eleventy --serve",
    "build": "eleventy",
    "build:drafts": "DRAFTS=true eleventy",
    "build:style1": "STYLE=1 eleventy",
    "build:style2": "STYLE=2 eleventy",
    "build:style3": "STYLE=3 eleventy",
    "build:style4": "STYLE=4 eleventy",
    "build:style5": "STYLE=5 eleventy"
  },
  "keywords": ["data-flow-map", "book", "eleventy"],
  "author": "Nick Ryberg",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1"
  }
}
